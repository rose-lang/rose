{
  "name": "@rose-lang/wasm",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/wbg/rose_web.js",
      "browser": "./dist/browser.js",
      "default": "./dist/index.js"
    },
    "./*": "./dist/bindings/*.js"
  },
  "scripts": {
    "build": "cp browser.js dist && esbuild index.js --outfile=dist/index.js --platform=neutral --bundle --loader:.wasm=binary --define:import.meta.url=null --sourcemap"
  }
